{
  "model_name": "Sales_Model", 
  "data_source": "sales_data.csv",
  "last_updated": "2023-11-15",
  "dimensions": [
    {
      "name": "Date",
      "type": "DATE",
      "hierarchy": ["Year", "Quarter", "Month"],
      "description": "Sales transaction date"
    },
    {
      "name": "Region",
      "type": "GENERIC",
      "values": ["North America", "Europe", "Asia", "South America"],
      "description": "Geographic sales region"
    },
    {
      "name": "Product_Category",
      "type": "GENERIC",
      "values": ["Technology", "Furniture", "Office Supplies"],
      "description": "Category of product sold"
    },
    {
      "name": "Sales_Stage",
      "type": "GENERIC",
      "values": ["Lead", "Qualified Lead", "Proposal", "Negotiation", "Closed Won", "Closed Lost"],
      "description": "Stage in sales pipeline"
    }
  ],
  "measures": [
    {
      "name": "Sales_Amount",
      "type": "AMOUNT",
      "format": "Currency (USD)",
      "aggregation": "SUM",
      "description": "Total value of sales"
    },
    {
      "name": "Quantity",
      "type": "NUMBER",
      "aggregation": "SUM",
      "description": "Number of units sold"
    },
    {
      "name": "Profit",
      "type": "AMOUNT",
      "format": "Currency (USD)",
      "aggregation": "SUM",
      "description": "Profit from sales"
    },
    {
      "name": "Profit_Margin",
      "type": "CALCULATED",
      "formula": "Profit / Sales_Amount",
      "format": "Percentage",
      "description": "Profit as percentage of sales"
    }
  ],
  "predictive_features": {
    "time_series_forecasting": true,
    "target_measure": "Sales_Amount",
    "date_dimension": "Date"
  }
}
